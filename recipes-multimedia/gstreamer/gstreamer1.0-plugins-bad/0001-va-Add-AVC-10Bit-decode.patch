---
 sys/va/gstvah264dec.c | 4 ++--
 sys/va/gstvaprofile.c | 2 ++
 2 files changed, 4 insertions(+), 2 deletions(-)

diff --git a/sys/va/gstvah264dec.c b/sys/va/gstvah264dec.c
index c4a149434..658dbfb75 100644
--- a/sys/va/gstvah264dec.c
+++ b/sys/va/gstvah264dec.c
@@ -574,8 +574,8 @@ static const struct
   P (MAIN, Main),
   /* P (EXTENDED, ), */
   P (HIGH, High),
-  /* P (HIGH10, ), */
-  /* P (HIGH_422, ), */
